{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "INTERCAL",
	"patterns": [
		{
			"include": "#comments"
		}
	],
	"repository": {
		"comments": {
			"patterns": [{
				"name": "comment.line.other.intercal",
				"match": "(DO|PLEASE|PLEASE(N'T| NOT) DO)(N'T| NOT).*"
			}]
		}
	},
	"scopeName": "source.intercal"
}